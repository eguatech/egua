função tSesenão(){
  escreva("----------------------------");
  escreva("Iniciando Teste: Se - Se Não");
  escreva("----------------------------");
  var a = 1;
  var b = 1;
  se (a == b)
    retorna("Se - Se não: OK!");
  senão {
    erros = erros + 1;
    retorna("Se - Se não: ERRO!");
  }
}

função tEnquanto(){
  escreva("------------------------");
  escreva("Iniciando Teste: Enquanto");
  escreva("------------------------");
  var a = 0;
  enquanto(a<5){
    a = a + 1;
  }
  se (a == 5)
    retorna("Enquanto: OK!");
  senão {
    erros = erros + 1;
    retorna("Enquanto: ERRO!");
  }
  escreva(a);
}

função tPara(){
  escreva("--------------------");
  escreva("Iniciando Teste: Para");
  escreva("--------------------");
  var a = 0;
  para (var i = 0; i < 5; i = i + 1) {
  a = a + 1;
  }
  se (a == 5)
    retorna("Para: OK!");
  senão {
    erros = erros + 1;
    retorna("Para: ERRO!");
  }
}

função tFacaEnquanto(){
  escreva("-----------------------------");
  escreva("Iniciando Teste: Faça - Enquanto");
  escreva("-----------------------------");
  var a = 0;
  fazer {
    a = a + 1;
  } enquanto(a<5){
  }
  se (a == 5)
    retorna("Faça - Enquanto: OK!");
  senão {
    erros = erros + 1;
    retorna("Faça - Enquanto: ERRO!");
  }
}

função tCasoEscolha(){
  escreva("--------------------");
